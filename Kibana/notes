## Kibana Canvas
Workpad, page, Elements

wget https://raw.githubusercontent.com/elastic/examples/master/Common%20Data%20Formats/nginx_json_logs/nginx_json_logs
 
awk '{print "{\"index\":{}}\n" $0}' nginx_json_logs > nginx_json_logs_bulk
 
 
curl -u elastic:fRMrgyuBtROlOrrtZ0UvgttV -H "Content-Type:application/json" -XPUT "https://guide-to-es.es.us-central1.gcp.cloud.es.io/nginx" \
-d '{
   "settings": {
       "number_of_shards": 1,
       "number_of_replicas": 0
   },
   "mappings": {
       "properties": {
           "time": {"type":"date","format":"dd/MMM/yyyy:HH:mm:ss Z"},
           "response": {"type":"keyword"}
       }
   }
}'
 
curl -u elastic:fRMrgyuBtROlOrrtZ0UvgttV --header 'Content-Type: application/x-ndjson' -XPOST "https://guide-to-es.es.us-central1.gcp.cloud.es.io/nginx/_bulk" --data-binary '@nginx_json_logs_bulk' | jq '.errors'

--on kibana devTools
GET /_sql?format=txt
{
 "query": """
 SELECT sum(bytes) as total_transferred from nginx
 group by remote_ip
 order by total_transferred desc
 """ 
}
